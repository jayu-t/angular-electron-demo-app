{
  "name": "angular-electron-demo-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "electron:ng-build": "ng run angular-electron-demo-app:electron-build:development",
    "electron:ng-serve": "ng run angular-electron-demo-app:electron-serve:development",
    "electron:build": "tsc -p electron.tsconfig.json",
    "electron:start": "npm run electron:ng-build && npm run electron:build && electron dist/electron-app/electron-main/main.js",
    "nest:start": "nest start --path electron.tsconfig.json --config nest-cli.json",
    "nest:build": "nest build",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.0",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/forms": "^16.1.0",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/router": "^16.1.0",
    "@nestjs/common": "^10.2.0",
    "@nestjs/core": "^10.2.0",
    "@nestjs/platform-express": "^10.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "wait-on": "^7.0.1",
    "winston": "^3.10.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^16.0.0",
    "@angular-devkit/build-angular": "^16.1.8",
    "@angular/cli": "~16.1.8",
    "@angular/compiler-cli": "^16.1.0",
    "@nestjs/cli": "^10.1.12",
    "@nestjs/microservices": "^10.2.0",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~4.3.0",
    "@types/supertest": "^2.0.12",
    "electron": "^26.0.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "supertest": "^6.3.3",
    "typescript": "~5.1.3"
  }
}
